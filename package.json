{
  "name": "his-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run clean && npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate:create": "sequelize migration:generate --name update_users_table",
    "migrate:down": "sequelize db:migrate:undo",
    "migrate:up": "npx sequelize-cli db:migrate",
    "seed": "tsx src/scripts/importMasterData.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "@types/log4js": "^0.0.33",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.3"
  },
  "_moduleAliases": {
    "@plugins": "dist/plugins",
    "@config": "dist/config",
    "@jobs": "dist/jobs",
    "@scripts": "dist/scripts",
    "@": "dist",
    "@test": "dist/test",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares"
  },
  "xo": {
    "extends": ".eslintrc.json"
  }
}
